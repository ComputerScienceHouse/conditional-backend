  "query": "\n        SELECT\n            mp.id,\n            u.id uid,\n            mp.name,\n            mp.description,\n            mp.status as \"status: db::MajorProjectStatusEnum\"\n        FROM major_project mp \n        LEFT JOIN \"user\" u\n        \tON u.id = mp.uid\n        WHERE date > $1\n        order by date",
